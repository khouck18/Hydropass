{
  "extends": ["react-app", "react-app/jest", "prettier"],
  "rules": {
    "semi": ["error", "always"],
    "eqeqeq": "error",
    "quotes": ["error", "double"],
    "no-console": "warn",
    "no-const-assign": "error",
    "no-debugger": "error",
    "no-duplicate-imports": "error",
    "arrow-spacing": "warn",
    "comma-spacing": ["error", { "before": false, "after": true }]
  }
}
